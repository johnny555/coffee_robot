<!--
   Run the 3d fiducial localization for a raspi

  Fiducials are 0.136, which needs to be passed down through the mapping stack. 

-->
<launch>

  <!-- camera -->
  <include file="$(find raspicam_node)/launch/camerav2_1280x960_10fps.launch" >
	<arg name="enable_raw" value="true" />
  </include>
  
</launch>
