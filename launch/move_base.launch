<launch>


<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find coffee_robot)/param/costmap_common_param.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find coffee_robot)/param/costmap_common_param.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find coffee_robot)/param/local_costmap_param.yaml" command="load" />
    <rosparam file="$(find coffee_robot)/param/global_costmap_param.yaml" command="load" />
    <rosparam file="$(find coffee_robot)/param/base_local_planner_param.yaml" command="load" />

  </node>


</launch>

